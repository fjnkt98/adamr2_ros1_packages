<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="caster_gazebo" params="prefix">
    <gazebo reference="${prefix}_caster_support_link">
      <material>
        Gazebo/Red
      </material>
      <mu1 value="0.5"/>
      <mu2 value="0.5"/>
    </gazebo>

    <gazebo reference="${prefix}_caster_wheel_link">
      <material>
        Gazebo/Red
      </material>
      <mu1 value="0.5"/>
      <mu2 value="0.5"/>
    </gazebo>
  </xacro:macro>
</robot>